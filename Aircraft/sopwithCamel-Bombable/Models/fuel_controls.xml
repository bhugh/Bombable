<?xml version="1.0"?>
<PropertyList> 

	<path>fuel_controls.ac</path>
	<texture-path>Textures</texture-path>
	
	<animation>
 		<type>rotate</type>
 		<object-name>throttle-lever</object-name>
		<property>controls/engines/engine[0]/throttle</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-45</dep>
			</entry>
			<entry>
				<ind>0.5</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>45</dep>
			</entry>
		</interpolation>
 		<center>
 			<x-m>0</x-m>
 			<y-m>0</y-m>
 			<z-m>0</z-m>
 		</center>
 		<axis>
 			<x>0</x>
 			<y>-1</y>
 			<z>0</z>
 		</axis>
 	</animation>
	
	<animation>
 		<type>rotate</type>
 		<object-name>throttle_pushrod</object-name>
		<property>controls/engines/engine[0]/throttle</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-45</dep>
			</entry>
			<entry>
				<ind>0.5</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>45</dep>
			</entry>
		</interpolation>
 		<center>
 			<x-m>-0.0000</x-m>
 			<y-m>0</y-m>
 			<z-m>0.0492</z-m>
 		</center>
 		<axis>
 			<x>0</x>
 			<y>1</y>
 			<z>0</z>
 		</axis>
 	</animation>
	
	<animation>
 		<type>rotate</type>
 		<object-name>mixture_lever</object-name>
		<property>controls/engines/engine/mixture</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-15</dep>
			</entry>
			<entry>
				<ind>0.5</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>15</dep>
			</entry>
		</interpolation>
 		<center>
 			<x-m>0</x-m>
 			<y-m>0</y-m>
 			<z-m>0</z-m>
 		</center>
 		<axis>
 			<x>0</x>
 			<y>-1</y>
 			<z>0</z>
 		</axis>
 	</animation>
	
	<animation>
 		<type>rotate</type>
 		<object-name>mixture_pushrod</object-name>
		<property>controls/engines/engine/mixture</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-15</dep>
			</entry>
			<entry>
				<ind>0.5</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>15</dep>
			</entry>
		</interpolation>
 		<center>
 			<x-m>-0.0000</x-m>
 			<y-m>0</y-m>
 			<z-m>0.0226</z-m>
 		</center>
 		<axis>
 			<x>0</x>
 			<y>1</y>
 			<z>0</z>
 		</axis>
 	</animation>
	
	<animation>
 		<type>rotate</type>
 		<object-name>mixture_rocker_group</object-name>
		<property>controls/engines/engine/mixture</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-15</dep>
			</entry>
			<entry>
				<ind>0.5</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>15</dep>
			</entry>
		</interpolation>
 		<center>
 			<x-m>0.0616</x-m>
 			<y-m>0</y-m>
 			<z-m>-0.0202</z-m>
 		</center>
 		<axis>
 			<x>0</x>
 			<y>-1</y>
 			<z>0</z>
 		</axis>
 	</animation>
	
	<animation>
 		<type>rotate</type>
 		<object-name>mixture_link</object-name>
		<property>controls/engines/engine/mixture</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-15</dep>
			</entry>
			<entry>
				<ind>0.5</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>15</dep>
			</entry>
		</interpolation>
 		<center>
 			<x-m>0.0724</x-m>
 			<y-m>0</y-m>
 			<z-m>-0.0209</z-m>
 		</center>
 		<axis>
 			<x>0</x>
 			<y>1</y>
 			<z>0</z>
 		</axis>
 	</animation>

	<animation> 
		<type>select</type>
		<object-name>stuff_to_hide</object-name>
		<condition>
			<property>sim/model/camel/extra-details</property>
		</condition>
	</animation> 

	<!-- *** LoD *** -->
	<animation> 
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>
</PropertyList>
