<?xml version="1.0"?>
<PropertyList>

	<path>clerget.ac</path>
	<texture-path>Textures</texture-path>

	<animation>
		<type>spin</type>
		<object-name>complete_engine</object-name>
		<property>engines/engine/rpm</property>
		<factor>1</factor>
		<offset>100</offset>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- ***** Propeller ***** -->
	<animation>
		<type>select</type>
		<object-name>Blade-1</object-name>
		<object-name>Blade-2</object-name>
		<condition>
			<less-than>
				<property>engines/engine[0]/rpm</property>
				<value>850</value>
			</less-than>
		</condition>
	</animation>



	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>700</max-m>
	</animation>
</PropertyList>
