<?xml version="1.0"?>

<PropertyList>

	<path>A-10-elec-pwr-ctrl.ac</path>

	<!-- apu-gen-power -->
	<animation>
		<object-name>apu-gen-pwr-switch</object-name>
		<type>rotate</type>
		<property>controls/APU/generator</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep>30</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.0275</x-m>
			<z-m>0.0031</z-m>
		</center>
		<axis><y>-1</y></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>apu-gen-pwr</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/APU/generator</property>
			</binding>
		</action>
	</animation>


	<!-- inverter-stby -->
	<animation>
		<object-name>inverter-stby-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/switches/inverter</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
			<entry><ind>2</ind><dep>30</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.0275</x-m>
			<z-m>0.0031</z-m>
		</center>
		<axis><y>-1</y></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>inverter-stby-up</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>electrical.inverter_switch(1)</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>inverter-stby-down</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>electrical.inverter_switch(-1)</script>
			</binding>
		</action>
	</animation>


	<!-- l-ac-gen-power -->
	<animation>
		<object-name>l-ac-gen-switch</object-name>
		<type>rotate</type>
		<property>controls/electric/engine[0]/generator</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep>30</dep></entry>
		</interpolation>
		<center>
			<x-m>0.0191</x-m>
			<z-m>0.0031</z-m>
		</center>
		<axis><y>-1</y></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>l-ac-gen</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/electric/engine[0]/generator</property>
			</binding>
		</action>
	</animation>

	<!-- r-ac-gen-pwr -->
	<animation>
		<object-name>r-ac-gen-switch</object-name>
		<type>rotate</type>
		<property>controls/electric/engine[1]/generator</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep>30</dep></entry>
		</interpolation>
		<center>
			<x-m>0.0191</x-m>
			<z-m>0.0031</z-m>
		</center>
		<axis><y>-1</y></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>r-ac-gen</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/electric/engine[1]/generator</property>
			</binding>
		</action>
	</animation>

	<!-- battery-pwr -->
	<animation>
		<object-name>battery-pwr-switch</object-name>
		<type>rotate</type>
		<property>controls/electric/battery-switch</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep>30</dep></entry>
		</interpolation>
		<center>
			<x-m>0.0191</x-m>
			<z-m>0.0031</z-m>
		</center>
		<axis><y>-1</y></axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>battery-pwr</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/electric/battery-switch</property>
			</binding>
		</action>
	</animation>

	<!-- Lighting -->
	<animation>
		<object-name>mount</object-name>
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.385</red>
			<green>0.168</green>
			<blue>0.07</blue>
		</emission>
	</animation>

	<animation> 
		<object-name>switches</object-name>
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.385</red>
			<green>0.168</green>
			<blue>0.07</blue>
		</emission>
	</animation>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>15</max-m>
	</animation>

</PropertyList>
