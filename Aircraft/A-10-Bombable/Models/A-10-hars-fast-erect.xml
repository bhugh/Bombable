<?xml version="1.0"?>

<PropertyList>

	<path>A-10-hars-fast-erect.ac</path>

	<!-- lighting -->

	<animation>
		<object-name>mount</object-name>
		<object-name>button</object-name>
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.385</red>
			<green>0.168</green>
			<blue>0.07</blue>
		</emission>
	</animation>


	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>
