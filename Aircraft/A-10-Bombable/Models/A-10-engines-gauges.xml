        <entry><ind>0.2310</ind><dep>164</dep></entry><!--25% thrust-->
        <entry><ind>0.9870</ind><dep>295</dep></entry><!--max thrust-->
      </interpolation>
      <center>
       <x-m>0.0047</x-m>
       <y-m>-0.0263</y-m>
       <z-m>0.0547</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>

