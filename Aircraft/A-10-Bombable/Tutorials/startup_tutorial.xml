<PropertyList>
	<name>Engines Start</name>
	<description>This tutorial teaches the A-10 engines start procedure</description>
	<interval>0</interval>

	<init>					
		<set>
			<property>sim/current-view/view-number</property>
			<value>0</value>
		</set>
		<set>
			<property>controls/gear/brake-parking</property>
			<value>1</value>
		</set>
		<set>
			<property>controls/flight/flaps</property>
			<value>0.0</value>
		</set>
		<set>
			<property>controls/electric/battery-switch</property>
			<value>0</value>
		</set>
		<set>
			<property>sim/model/A-10/controls/engines/engine[0]/starter-switch-position</property>
			<value>1</value>
		</set>
		<set>
			<property>sim/model/A-10/controls/engines/engine[1]/starter-switch-position</property>
			<value>1</value>
		</set>
		<set>
			<property>sim/model/A-10/controls/fuel/cross-feed-sw</property>
			<value>false</value>
		</set>
		<set>
			<property>controls/APU/off-start-switch</property>
			<value>false</value>
		</set>
		<set>
			<property>controls/electric/APU-generator</property>
			<value>false</value>
		</set>
		<set>
			<property>controls/electric/engine[0]/generator</property>
			<value>false</value>
		</set>
		<set>
			<property>controls/electric/engine[1]/generator</property>
			<value>false</value>
		</set>
		<set>
			<property>controls/engines/engine[0]/cutoff</property>
			<value>true</value>
		</set>
		<set>
			<property>controls/engines/engine[0]/throttle</property>
			<value>0.0</value>
		</set>
		<set>
			<property>sim/model/A-10/controls/engines/engine[0]/throttle</property>
			<value>0.0</value>
		</set>
		<set>
			<property>engines/engine[0]/out-of-fuel</property>
			<value>true</value>
		</set>
		<set>
			<property>sim/model/A-10/engines/engine[0]/n1</property>
			<value>0.0</value>
		</set>
		<set>
			<property>sim/model/A-10/engines/engine[0]/n2</property>
			<value>0.0</value>
		</set>
		<set>
			<property>controls/engines/engine[1]/cutoff</property>
			<value>true</value>
		</set>
		<set>
			<property>controls/engines/engine[1]/throttle</property>
			<value>0.0</value>
		</set>
		<set>
			<property>sim/model/A-10/controls/engines/engine[1]/throttle</property>
			<value>0.0</value>
		</set>
		<set>
			<property>engines/engine[1]/out-of-fuel</property>
			<value>true</value>
		</set>
		<set>
			<property>sim/model/A-10/engines/engine[1]/n1</property>
			<value>0.0</value>
		</set>
		<set>
			<property>sim/model/A-10/engines/engine[1]/n2</property>
			<value>0.0</value>
		</set>
		<set>
			<property>systems/A-10-fuel/collector-tank[0]</property>
			<value>0.0</value>
		</set>
		<set>
			<property>systems/A-10-fuel/collector-tank[1]</property>
			<value>0.0</value>
		</set>
		<set>
			<property>systems/A-10-fuel/apu-collector-tank</property>
			<value>0.0</value>
		</set>
	</init>

	<step>
		<message>
			First, ensure battery is switched on.
			Use the battery switch on the electrical control panel.
		</message>
		<view>
			<heading-offset-deg>327.7</heading-offset-deg>
			<pitch-offset-deg>-40.6</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>43.1</field-of-view>
		</view>
		<marker>
			<x-m>2.6453</x-m>
			<y-m>0.5333</y-m>
			<z-m>0.7689</z-m>
			<scale>1.5689</scale>
		</marker>
		<exit>
			<condition>
				<property>controls/electric/battery-switch</property>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			Switch on the cross bleed valve, to prevent
			load inbalance between right and left wing tanks.
		</message>
		<view>
			<heading-offset-deg>28.5</heading-offset-deg>
			<pitch-offset-deg>-50</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<marker>
			<x-m>2.5209</x-m>
			<y-m>-0.3680</y-m>
			<z-m>0.8378</z-m>
			<scale>1.5689</scale>
		</marker>
		<exit>
			<condition>
				<property>sim/model/A-10/controls/fuel/cross-feed-sw</property>
			</condition>
		</exit>
	</step>

	<step>
		<message>You can now start the APU.</message>
		<view>
			<heading-offset-deg>28.5</heading-offset-deg>
			<pitch-offset-deg>-50</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<marker>
			<x-m>2.7191</x-m>
			<y-m>-0.3147</y-m>
			<z-m>0.7538</z-m>
			<scale>1.7289</scale>
		</marker>
		<exit>
			<condition>
				<property>controls/APU/off-start-switch</property>
			</condition>
		</exit>
	</step>

	<step>
		<message>Let the APU rpm raise to 60% RPM.</message>
		<view>
			<heading-offset-deg>342.2</heading-offset-deg>
			<pitch-offset-deg>-34.1</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<marker>
			<x-m>2.4262</x-m>
			<y-m>0.2133</y-m>
			<z-m>0.9516</z-m>
			<scale>3.0</scale>
		</marker>
		<exit>
			<condition>
				<greater-than>
					<property>sim/model/A-10/systems/apu/rpm-norm</property>
					<value>60</value>
				</greater-than>
			</condition>
		</exit>
	</step>

	<step>
		<message>Now engage the APU generator.</message>
		<view>
			<heading-offset-deg>342.2</heading-offset-deg>
			<pitch-offset-deg>-34.1</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<marker>
			<x-m>2.5982</x-m>
			<y-m>0.4644</y-m>
			<z-m>0.7787</z-m>
			<scale>1.5689</scale>
		</marker>
		<exit>
			<condition>
				<greater-than>
					<property>systems/electrical/APU-gen-volts</property>
					<value>23</value>
				</greater-than>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			Near 100% rpm, the APU will provide enough power and low pressure output
			to start an engine.
		</message>
		<view>
			<heading-offset-deg>342.2</heading-offset-deg>
			<pitch-offset-deg>-34.1</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<exit>
			<condition>
				<greater-than>
					<property>sim/model/A-10/systems/apu/rpm-norm</property>
					<value>95</value>
				</greater-than>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			Now we can launch the start cycle of the left engine.
			Move the left throttle from the OFF position to the IDLE position.
			To do this you have to click on it (move your joystick throttle axis is not sufficient).
		</message>
		<view>
			<heading-offset-deg>7.4</heading-offset-deg>
			<pitch-offset-deg>-46.8</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<marker>
			<x-m>2.7089</x-m>
			<y-m>-0.3907</y-m>
			<z-m>0.7458</z-m>
			<scale>1.2133</scale>
		</marker>
		<exit>
			<condition>
				<greater-than>
					<property>sim/model/A-10/controls/engines/engine[0]/throttle</property>
					<value>0.029</value>
				</greater-than>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			Wait the end of the left engine start cycle.
		</message>
		<view>
			<heading-offset-deg>342.2</heading-offset-deg>
			<pitch-offset-deg>-34.1</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<exit>
			<condition>
				<not>
					<property>systems/bleed-air/ats-valve[0]</property>
				</not>
			</condition>
		</exit>
	</step>

	<step>
		<message>
			Fine, we can now launch the start cycle of the right engine.
			Move the right throttle from the OFF position to the IDLE position.
		</message>
		<view>
			<heading-offset-deg>7.4</heading-offset-deg>
			<pitch-offset-deg>-46.8</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<marker>
			<x-m>2.7089</x-m>
			<y-m>-0.3560</y-m>
			<z-m>0.7458</z-m>
			<scale>1.2133</scale>
		</marker>
		<exit>
			<condition>
				<greater-than>
					<property>sim/model/A-10/controls/engines/engine[1]/throttle</property>
					<value>0.029</value>
				</greater-than>
			</condition>
		</exit>
	</step>

	<step>
		<message>
			Wait the end of the right engine start cycle.
		</message>
		<view>
			<heading-offset-deg>342.2</heading-offset-deg>
			<pitch-offset-deg>-34.1</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<exit>
			<condition>
				<not>
					<property>systems/bleed-air/ats-valve[1]</property>
				</not>
			</condition>
		</exit>
	</step>

