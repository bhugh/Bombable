	<step>
		<message>Shut off the APU generator</message>
		<view>
			<heading-offset-deg>350.5</heading-offset-deg>
			<pitch-offset-deg>-47.8</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<marker>
			<x-m>2.5982</x-m>
			<y-m>0.4644</y-m>
			<z-m>0.7787</z-m>
			<scale>1.5689</scale>
		</marker>
		<exit>
			<condition>
				<not>
					<property>controls/electric/APU-generator</property>
				</not>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>and the APU.</message>
		<view>
			<heading-offset-deg>28.5</heading-offset-deg>
			<pitch-offset-deg>-50</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<marker>
			<x-m>2.7191</x-m>
			<y-m>-0.3147</y-m>
			<z-m>0.7538</z-m>
			<scale>1.7289</scale>
		</marker>
		<exit>
			<condition>
				<not>
					<property>controls/APU/off-start-switch</property>
				</not>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>
			Switch the battery off.
		</message>
		<view>
			<heading-offset-deg>327.7</heading-offset-deg>
			<pitch-offset-deg>-40.6</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>43.1</field-of-view>
		</view>
		<marker>
			<x-m>2.6453</x-m>
			<y-m>0.5333</y-m>
			<z-m>0.7689</z-m>
			<scale>1.5689</scale>
		</marker>
		<exit>
			<condition>
				<not>
					<property>controls/electric/battery-switch</property>
				</not>
			</condition>
		</exit>
	</step>
	
	<step>
		<message>Finally switch off the cross bleed valve.</message>
		<view>
			<heading-offset-deg>28.5</heading-offset-deg>
			<pitch-offset-deg>-50</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
		<marker>
			<x-m>2.5209</x-m>
			<y-m>-0.3680</y-m>
			<z-m>0.8378</z-m>
			<scale>1.5689</scale>
		</marker>
		<exit>
			<condition>
				<not>
					<property>sim/model/A-10/controls/fuel/cross-feed-sw</property>
				</not>
			</condition>
		</exit>
	</step>
	
	<end>
		<message>Congratulations! You have reached the end of this tutorial without
		starting the ejection seat sequence.</message>
		<view>
			<heading-offset-deg>0.0</heading-offset-deg>
			<pitch-offset-deg>-10.0</pitch-offset-deg>
			<roll-offset-deg>0.0</roll-offset-deg>
			<x-offset-m>0.0</x-offset-m>
			<y-offset-m>1.5</y-offset-m>
			<z-offset-m>3.2</z-offset-m>
			<field-of-view>55.0</field-of-view>
		</view>
	</end>

</PropertyList>
