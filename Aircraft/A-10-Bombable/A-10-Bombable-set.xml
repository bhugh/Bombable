		<chase-distance-m type="double" archive="y">-30.0</chase-distance-m>

		<tutorials>
			<tutorial include="Tutorials/startup_tutorial.xml"/>
		</tutorials>

		<view n="0">
			<name>Cockpit View</name>
			<type>lookfrom</type>         
			<internal archive="y">true</internal>
			<config>
				<x-offset-m archive="y">0.0</x-offset-m>
				<y-offset-m archive="y">1.50</y-offset-m>
				<z-offset-m archive="y">3.20</z-offset-m>
				<pitch-offset-deg>-15</pitch-offset-deg>
				<default-field-of-view-deg>55</default-field-of-view-deg>
				<limits>
					<enabled type="bool">true</enabled>
					<left>
						<heading-max-deg>155</heading-max-deg>
						<x-offset-threshold-deg>65</x-offset-threshold-deg>
						<x-offset-max-m>0.29</x-offset-max-m>
					</left>
					<right>
						<heading-max-deg>155</heading-max-deg>
						<x-offset-threshold-deg>65</x-offset-threshold-deg>
						<x-offset-max-m>0.29</x-offset-max-m>
					</right>
				</limits>
			</config>
		</view>
		<view n="1">
			<config>
				<target-z-offset-m archive="y" type="double">3</target-z-offset-m>
				<pitch-offset-deg>0</pitch-offset-deg>
