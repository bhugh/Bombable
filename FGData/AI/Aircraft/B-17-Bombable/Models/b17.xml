  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>direction</object-name>
    <property>controls/flight/rudder</property>
    <factor> 25 </factor>
    <axis>
      <x1-m> 8.718 </x1-m>
      <y1-m> 0.000 </y1-m>
      <z1-m> 1.000 </z1-m>
      <x2-m> 8.861 </x2-m>
      <y2-m> 0.000 </y2-m>
      <z2-m> 3.546 </z2-m>
    </axis>
  </animation>

