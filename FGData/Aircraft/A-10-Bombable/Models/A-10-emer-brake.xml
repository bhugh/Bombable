<?xml version="1.0"?>


<PropertyList>

  <path>A-10-emer-brake.ac</path>

  <animation>
    <object-name>emer-brake</object-name>
    <type>translate</type>
 	  <property>controls/gear/brake-parking</property>
    <factor>0.05</factor>
    <center>
      <x-m>2.5177</x-m>
      <y-m>-0.5059</y-m>
      <z-m>0.8366</z-m>
	  </center>
	  <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
	  </axis>
  </animation>


<!-- lighting -->

  <animation> 
    <object-name>emer-brake</object-name>
    <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
      <red>0.20</red>
      <green>0.10</green>
      <blue>0.05</blue>
    </emission>
  </animation>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>5</max-m>
  </animation>

</PropertyList>
